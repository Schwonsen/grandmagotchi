<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/kitchen_gradient"
    tools:context=".FragmentKitchen" >

    <!-- TODO: Update blank fragment layout -->
    
        <ImageView android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:src="@drawable/kitchen"
        android:layout_marginBottom="20dp"
        android:id="@+id/kitchen" android:contentDescription="@string/kitchen"/>
        
    <ImageButton
        android:id="@+id/btn_food"
        android:layout_width="71dp"
        android:layout_height="60dp"
        android:layout_gravity="bottom|right"
        android:contentDescription="@string/btn_description_food"
        android:onClick="btnOnClickEat"
        android:src="@drawable/ic_action_eat" />
    <!-- android:background="@android:color/transparent" -->
    <!-- android:background="@android:color/transparent" -->

    <ImageButton
        android:id="@+id/btn_wash_dishes"
        android:layout_width="71dp"
        android:layout_height="60dp"
        android:layout_gravity="bottom|left"
        android:contentDescription="@string/btn_description_drink"
        android:onClick="btnOnClickWashDishes"
        android:src="@drawable/ic_action_dishes" />
